{
    "devDependencies": {
        "husky": "^9.1.5",
        "@commitlint/config-conventional": "^19.2.2",
        "@commitlint/cli": "^19.4.0"
    },
    "scripts": {
        "prepare": "husky",
        "verify-branch": "node scripts/verify-branch-name.js"
    },
    "husky": {
        "hooks": {
            "commit-msg": "pnpm exec commitlint --edit $1",
            "pre-push": "node scripts/verify-branch-name.js"
        }
    }
}